<?xml version="1.0" encoding="UTF-8"?>
<form>
   <title>Page - Page</title>
   <description/>
   <objectType>page</objectType>
   <content-type>/page/page</content-type>
   <imageThumbnail>undefined</imageThumbnail>
   <properties>
      <property>
         <name>display-template</name>
         <label>Display Template</label>
         <value>/templates/web/page.ftl</value>
         <type>template</type>
      </property>
      <property>
         <name>merge-strategy</name>
         <label>Merge Strategy</label>
         <value>inherit-levels</value>
         <type>string</type>
      </property>
      <property>
         <name>no-template-required</name>
         <label>No Template Required</label>
         <type>boolean</type>
         <value/>
      </property>
   </properties>
   <sections>
      <section>
         <title>Page Properties</title>
         <description/>
         <defaultOpen>true</defaultOpen>
         <fields>
            <field>
               <type>file-name</type>
               <id>file-name</id>
               <iceId/>
               <title>Page URL</title>
               <description/>
               <defaultValue/>
               <help/>
               <properties>
                  <property>
                     <name>size</name>
                     <value>50</value>
                     <type>int</type>
                  </property>
                  <property>
                     <name>maxlength</name>
                     <value>50</value>
                     <type>int</type>
                  </property>
                  <property>
                     <name>readonly</name>
                     <value/>
                     <type>boolean</type>
                  </property>
               </properties>
               <constraints/>
            </field>
            <field>
               <type>input</type>
               <id>internal-name</id>
               <iceId/>
               <title>Internal Name</title>
               <description/>
               <defaultValue/>
               <help/>
               <properties>
                  <property>
                     <name>size</name>
                     <value>50</value>
                     <type>int</type>
                  </property>
                  <property>
                     <name>maxlength</name>
                     <value>50</value>
                     <type>int</type>
                  </property>
               </properties>
               <constraints>
                  <constraint>
                     <name>required</name>
                     <value>true</value>
                     <type>boolean</type>
                  </constraint>
               </constraints>
            </field>
            <field>
               <type>page-nav-order</type>
               <id>placeInNav</id>
               <iceId/>
               <title>Place in Nav</title>
               <description/>
               <defaultValue/>
               <help/>
               <properties>
                  <property>
                     <name>readonly</name>
                     <value/>
                     <type>boolean</type>
                  </property>
               </properties>
               <constraints>
                  <constraint>
                     <name>required</name>
                     <value/>
                     <type>boolean</type>
                  </constraint>
               </constraints>
            </field>
            <field>
               <type>input</type>
               <id>title_t</id>
               <iceId/>
               <title>Title</title>
               <description/>
               <defaultValue/>
               <help/>
               <properties>
                  <property>
                     <name>size</name>
                     <value>50</value>
                     <type>int</type>
                  </property>
                  <property>
                     <name>maxlength</name>
                     <value>50</value>
                     <type>int</type>
                  </property>
                  <property>
                     <name>readonly</name>
                     <value/>
                     <type>boolean</type>
                  </property>
                  <property>
                     <name>tokenize</name>
                     <value>false</value>
                     <type>boolean</type>
                  </property>
               </properties>
               <constraints>
                  <constraint>
                     <name>required</name>
                     <value/>
                     <type>boolean</type>
                  </constraint>
                  <constraint>
                     <name>pattern</name>
                     <value/>
                     <type>string</type>
                  </constraint>
               </constraints>
            </field>
         </fields>
      </section>
      <section>
         <title>Section</title>
         <description/>
         <defaultOpen>false</defaultOpen>
         <fields>
            <field>
               <type>node-selector</type>
               <id>sectionComponents_o</id>
               <iceId>page-contents</iceId>
               <title>Section Components</title>
               <description/>
               <defaultValue/>
               <help/>
               <properties>
                  <property>
                     <name>minSize</name>
                     <value/>
                     <type>int</type>
                  </property>
                  <property>
                     <name>maxSize</name>
                     <value/>
                     <type>int</type>
                  </property>
                  <property>
                     <name>itemManager</name>
                     <value>components</value>
                     <type>datasource:item</type>
                  </property>
                  <property>
                     <name>readonly</name>
                     <value/>
                     <type>boolean</type>
                  </property>
                  <property>
                     <name>disableFlattening</name>
                     <value/>
                     <type>boolean</type>
                  </property>
                  <property>
                     <name>useSingleValueFilename</name>
                     <value/>
                     <type>boolean</type>
                  </property>
               </properties>
               <constraints>
                  <constraint>
                     <name>allowDuplicates</name>
                     <value>true</value>
                     <type>boolean</type>
                  </constraint>
               </constraints>
            </field>
         </fields>
      </section>
      <section>
         <title>Footer</title>
         <description/>
         <defaultOpen>false</defaultOpen>
         <fields>
            <field>
               <type>label</type>
               <id>mapCoordinatesInstructions_s</id>
               <iceId/>
               <title>Map Coordinates Instructions</title>
               <description/>
               <defaultValue/>
               <help/>
               <properties>
                  <property>
                     <name>text</name>
                     <value>To get a place's coordinates, you can right click any point in Google Maps and select "What's here?"</value>
                     <type>string</type>
                  </property>
               </properties>
               <constraints/>
            </field>
            <field>
               <type>label</type>
               <id>validLatitude_s</id>
               <iceId/>
               <title>Valid Latitude</title>
               <description/>
               <defaultValue/>
               <help/>
               <properties>
                  <property>
                     <name>text</name>
                     <value>Valid latitude is between -85 and 85</value>
                     <type>string</type>
                  </property>
               </properties>
               <constraints/>
            </field>
            <field>
               <type>input</type>
               <id>locationLatitude_s</id>
               <iceId/>
               <title>Location Latitude</title>
               <description/>
               <defaultValue>0</defaultValue>
               <help/>
               <properties>
                  <property>
                     <name>size</name>
                     <value>50</value>
                     <type>int</type>
                  </property>
                  <property>
                     <name>maxlength</name>
                     <value>50</value>
                     <type>int</type>
                  </property>
                  <property>
                     <name>readonly</name>
                     <value/>
                     <type>boolean</type>
                  </property>
                  <property>
                     <name>tokenize</name>
                     <value>false</value>
                     <type>boolean</type>
                  </property>
               </properties>
               <constraints>
                  <constraint>
                     <name>required</name>
                     <value>true</value>
                     <type>boolean</type>
                  </constraint>
                  <constraint>
                     <name>pattern</name>
                     <value>-?[0-9]+(\.[0-9]+)?</value>
                     <type>string</type>
                  </constraint>
               </constraints>
            </field>
            <field>
               <type>label</type>
               <id>validLongitude_s</id>
               <iceId/>
               <title>Valid Longitude</title>
               <description/>
               <defaultValue/>
               <help/>
               <properties>
                  <property>
                     <name>text</name>
                     <value>Valid longitude is between -180 and 180</value>
                     <type>string</type>
                  </property>
               </properties>
               <constraints/>
            </field>
            <field>
               <type>input</type>
               <id>locationLongitude_s</id>
               <iceId/>
               <title>Location Longitude</title>
               <description/>
               <defaultValue>0</defaultValue>
               <help/>
               <properties>
                  <property>
                     <name>size</name>
                     <value>50</value>
                     <type>int</type>
                  </property>
                  <property>
                     <name>maxlength</name>
                     <value>50</value>
                     <type>int</type>
                  </property>
                  <property>
                     <name>readonly</name>
                     <value/>
                     <type>boolean</type>
                  </property>
                  <property>
                     <name>tokenize</name>
                     <value>false</value>
                     <type>boolean</type>
                  </property>
               </properties>
               <constraints>
                  <constraint>
                     <name>required</name>
                     <value>true</value>
                     <type>boolean</type>
                  </constraint>
                  <constraint>
                     <name>pattern</name>
                     <value>-?[0-9]+(\.[0-9]+)?</value>
                     <type>string</type>
                  </constraint>
               </constraints>
            </field>
         </fields>
      </section>
   </sections>
   <datasources>
      <datasource>
         <type>video-desktop-upload</type>
         <id>uploadVideo</id>
         <title>Upload Video</title>
         <interface>video</interface>
         <properties>
            <property>
               <name>repoPath</name>
               <value>/static-assets/video</value>
               <type>undefined</type>
            </property>
         </properties>
      </datasource>
      <datasource>
         <type>video-browse-repo</type>
         <id>browseExistingVideo</id>
         <title>Browse Existing Video</title>
         <interface>video</interface>
         <properties>
            <property>
               <name>repoPath</name>
               <value>/static-assets/video</value>
               <type>undefined</type>
            </property>
         </properties>
      </datasource>
      <datasource>
         <type>shared-content</type>
         <id>components</id>
         <title>Components</title>
         <interface>item</interface>
         <properties>
            <property>
               <name>repoPath</name>
               <value>/site/components/sections</value>
               <type>undefined</type>
            </property>
            <property>
               <name>browsePath</name>
               <value>/site/components/sections</value>
               <type>undefined</type>
            </property>
            <property>
               <name>type</name>
               <value>/component/section</value>
               <type>undefined</type>
            </property>
         </properties>
      </datasource>
   </datasources>
</form>
